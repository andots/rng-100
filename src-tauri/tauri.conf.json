{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "RNG-100",
  "identifier": "rng-100",
  "build": {
    "frontendDist": "../target-frontend/web",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "just webdev",
    "beforeBuildCommand": "just webbuild"
  },
  "app": {
    "windows": [
      {
        "title": "RNG-100",
        "width": 220,
        "height": 220,
        "minHeight": 220,
        "minWidth": 220,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
